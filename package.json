{
  "name": "htbhf-applicant-web-ui",
  "version_notice": "Patch versions are incremented by Travis (in tag_version.sh via .travis.yml) - never manually increment the patch version",
  "version": "0.1.555",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "10.x"
  },
  "scripts": {
    "build": "npm run build:sass",
    "build:sass": "node-sass src/web/assets/sass -o src/web/public/styles --output-style compressed",
    "coverage": "nyc report --reporter=lcov --reporter=text-lcov > coverage.lcov && codecov",
    "postinstall": "npm run build",
    "start": "node src/web",
    "start:dev": "npm run build && nodemon $NODE_DEBUG_OPTION src/web -e js,json,html,njk",
    "test": "cross-env LOG_LEVEL=silent SESSION_DETAILS_PORT=8081 npm-run-all build test:lint test:unit:coverage test:fail_if_test_only_found -p -r start test:session-details test:browser",
    "test:acceptance": "./ci_scripts/download_and_run_acceptance_tests.sh",
    "test:accessibility": "node 'src/test/a11y/run-test-suite.js'",
    "test:browser": "npm run test:wait && npm run test:acceptance && npm run test:wiremock:start && npm-run-all -s test:accessibility test:wiremock:stop",
    "test:integration": "node 'src/test/integration/run-test-suite.js'",
    "test:fail_if_test_only_found": "./npm_scripts/fail_if_test_only_found.sh",
    "test:lint": "standard",
    "test:session-details": "cross-env SESSION_DETAILS_PORT=8081 node 'src/test/session-details-provider/session-details-app.js'",
    "test:unit": "tape -r './src/test/unit/setup' 'src/@(web|config)/**/*.test.js'",
    "test:unit:coverage": "nyc npm run test:unit",
    "test:wait": "./npm_scripts/wait_for_app_to_start.sh",
    "test:wiremock:start": "./npm_scripts/start_wiremock.sh",
    "test:wiremock:stop": "./npm_scripts/stop_wiremock.sh",
    "watch:sass": "node-sass -w src/web/assets/sass -o src/web/public/styles"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "bluebird": "^3.5.5",
    "codecov": "^3.1.0",
    "cross-env": "^5.2.0",
    "nodemon": "^1.18.9",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.0.0",
    "pa11y": "^5.1.0",
    "proxyquire": "^2.1.0",
    "safe-regex": "^2.0.2",
    "sinon": "^7.2.2",
    "standard": "^12.0.1",
    "tap-spec": "^5.0.0",
    "tape": "^4.9.2"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "cfenv": "^1.2.2",
    "compression": "^1.7.3",
    "connect-redis": "^3.4.0",
    "csurf": "^1.9.0",
    "dotenv": "^6.2.0",
    "escape-html": "^1.0.3",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "express-validator": "^6.0.0",
    "govuk-frontend": "^3.2.0",
    "helmet": "^3.21.1",
    "http-status-codes": "^1.3.0",
    "i18next": "^13.1.4",
    "i18next-express-middleware": "^1.7.1",
    "i18next-node-fs-backend": "^2.1.3",
    "libphonenumber-js": "^1.7.19",
    "moment": "^2.24.0",
    "node-sass": "^4.13.1",
    "notifications-node-client": "^4.6.0",
    "nunjucks": "^3.2.0",
    "ramda": "^0.26.1",
    "redis": "^2.8.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "uuidv4": "^3.0.1",
    "winston": "^3.2.1"
  }
}
