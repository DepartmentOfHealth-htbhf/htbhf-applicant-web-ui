{% extends "./page.njk" %}
{% from "button/macro.njk" import govukButton %}

{% block pageContent %}
  <form method="post" action="{{ form.action }}">
    <input type="hidden" name="_csrf" value="{{ csrfToken }}">
    {% block formContent %}{% endblock %}
    {{ govukButton({
      text: "Save and continue"
    }) }}
  </form>
{% endblock %}
